{
	"manifest_version": 2,
	"name": "Simple clock",
	"version": "1.0",
	"description": "First chrome extension",
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "image/icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "images/icon19.png",
			"38": "images/icon38.png"
		},
		"default_title": "My clock",
		"default_popup": "popup.html"
	}
}